{
  "name": "@bodiless/gatsby-starter-bodiless",
  "version": "0.3.1",
  "description": "This is a Gatsby starter for a bodiless website",
  "author": "e2thex <esummerfield@phase2technology.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "private": true,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@bodiless/accessibility": "^0.3.1",
    "@bodiless/backend": "^0.3.1",
    "@bodiless/card": "^0.3.1",
    "@bodiless/components": "^0.3.1",
    "@bodiless/components-ui": "^0.3.1",
    "@bodiless/core": "^0.3.1",
    "@bodiless/core-ui": "^0.3.1",
    "@bodiless/documentation": "^0.3.1",
    "@bodiless/fclasses": "^0.3.1",
    "@bodiless/gatsby-theme-bodiless": "^0.3.1",
    "@bodiless/layouts": "^0.3.1",
    "@bodiless/layouts-ui": "^0.3.1",
    "@bodiless/migration-tool": "^0.3.1",
    "@bodiless/navigation": "^0.3.1",
    "@bodiless/organisms": "^0.3.1",
    "@bodiless/richtext": "^0.3.1",
    "@bodiless/richtext-ui": "^0.3.1",
    "@bodiless/search": "^0.3.1",
    "@bodiless/ui": "^0.3.1",
    "autoprefixer": "^9.8.6",
    "cross-env": "^5.2.0",
    "docsify-cli": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gatsby": "~3.13.0",
    "gatsby-plugin-canonical-urls": "^3.6.0",
    "gatsby-plugin-sitemap": "^4.2.0",
    "lodash": "^4.17.19",
    "ora": "^4.0.2",
    "postcss": "^8.2.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^5.2.0",
    "tailwindcss": "^2.0.2"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "npm-run-all build:env-vars build:tailwind-configs build:lib build:doc build:search",
    "build:lib": "gatsby build",
    "build:doc": "bl-docs-build",
    "build:env-vars": "generate-env-vars",
    "build:tailwind-configs": "generate-tailwind-configs",
    "clean": "rimraf ./public/* && rimraf ./.cache",
    "dev-backend": "node ./node_modules/@bodiless/backend/src/server.js",
    "dev-frontend": "gatsby develop",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
    "prestart": "run-s build:env-vars build:tailwind-configs build:doc",
    "build:search": "npm run search-index",
    "start": "run-p -r dev-backend dev-frontend",
    "serve": "gatsby serve",
    "migrate": "cross-env DEBUG=migration_tool migration_tool",
    "migrate:debug": "cross-env DEBUG=migration_tool:* migration_tool",
    "migrate:postbuild": "cross-env DEBUG=migration_tool migration_tool postbuild public",
    "migrate:full": "npm run migrate && npm run build && npm run migrate:postbuild && npm run serve",
    "search-index": "create-search-index"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.19.0",
    "gatsby-plugin-compile-es6-packages": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "typescript": "^4.0.0"
  }
}
