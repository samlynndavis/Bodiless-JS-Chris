{
  "name": "@bodiless/organisms",
  "version": "1.0.0-beta.1",
  "description": "Basic components to build out sites.",
  "author": "Ivan Rudiuk <IRudiuk@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/doc",
    "site.tailwind.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run copy && tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api src",
    "lint": "tslint ./src/**/*.ts?x",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\"",
    "copy": "copyfiles -u 1 \"./src/**/*.{css,png}\" \"./lib/\""
  },
  "dependencies": {
    "@bodiless/accordion": "^1.0.0-beta.1",
    "@bodiless/bv": "^1.0.0-beta.1",
    "@bodiless/components": "^1.0.0-beta.1",
    "@bodiless/components-ui": "^1.0.0-beta.1",
    "@bodiless/core": "^1.0.0-beta.1",
    "@bodiless/fclasses": "^1.0.0-beta.1",
    "@bodiless/richtext": "^1.0.0-beta.1",
    "@bodiless/richtext-ui": "^1.0.0-beta.1",
    "@types/react-burger-menu": "^2.6.0",
    "@types/react-tag-autocomplete": "^6.0.0",
    "lodash": "^4.17.19",
    "react-tag-autocomplete": "^6.0.0",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^3.4.4"
  },
  "peerDependencies": {
    "mobx": "^6.0.0",
    "mobx-react": "^7.2.1",
    "react": "^17.0.2"
  }
}
