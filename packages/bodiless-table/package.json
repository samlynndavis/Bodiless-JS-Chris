{
  "name": "@bodiless/table",
  "version": "1.0.0-rc.25",
  "description": "Provide a Bodiless table component and HOC to make it editable",
  "author": "Erik Summerfield <esummer3@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/getDocs.js",
    "/doc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-p build:lib",
    "build:lib": "tsc -p ./tsconfig.json",
    "build:watch": "npm run build:lib -- --watch",
    "build:api-doc": "typedoc --options ../../typedoc.js --out doc/api ./src",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\""
  },
  "dependencies": {
    "@bodiless/cli": "^1.0.0-rc.25",
    "@bodiless/core": "^1.0.0-rc.25",
    "@bodiless/fclasses": "^1.0.0-rc.25",
    "lodash": "^4.17.19",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^5.0.3",
    "@types/lodash": "^4.14.158",
    "@types/uuid": "^3.4.4"
  }
}
