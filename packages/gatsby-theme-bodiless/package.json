{
  "name": "@bodiless/gatsby-theme-bodiless",
  "version": "0.3.7",
  "description": "A Gatsby theme for BodilessJS",
  "author": "Chris Oden <coden@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "sideEffects": false,
  "bin": {
    "generate-env-vars": "./bin/generate-env-vars",
    "generate-tailwind-configs": "./bin/generate-tailwind-configs",
    "bl-validate-logs": "./bin/bl-validate-logs",
    "bl-clear-logs": "./bin/bl-clear-logs"
  },
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "/bin",
    "dist",
    "index.js",
    "index.d.ts",
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "queries.jsx",
    "Logger.js",
    "src/no-scroll-settings.json",
    "build-css.js",
    "create-node.js",
    "create-redirect-alias.js",
    "tailwind.base.config.js",
    "/doc",
    "bodiless.content.json",
    "src/dist/tailwindcss/resolveConfig.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run copy && tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api src",
    "clean": "rimraf -rf \"dist\" && rimraf index* && rimraf \"doc/api\"",
    "copy": "copyfiles -u 1 \"./src/dist/**/*.{json,jpg,png}\" \".\""
  },
  "dependencies": {
    "@bodiless/accordion": "^0.3.7",
    "@bodiless/card": "^0.3.7",
    "@bodiless/components": "^0.3.7",
    "@bodiless/components-ui": "^0.3.7",
    "@bodiless/core": "^0.3.7",
    "@bodiless/core-ui": "^0.3.7",
    "@bodiless/fclasses": "^0.3.7",
    "@bodiless/layouts": "^0.3.7",
    "@bodiless/navigation": "^0.3.7",
    "@bodiless/organisms": "^0.3.7",
    "@bodiless/page": "^0.3.7",
    "@bodiless/ui": "^0.3.7",
    "@types/walk": "^2.3.0",
    "autoprefixer": "^10.0.0",
    "axios": "^0.21.0",
    "babel-plugin-preval": "5.0.0",
    "crypto-browserify": "^3.12.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "fast-glob": "^3.2.5",
    "find-up": "^4.1.0",
    "fs-extra": "^8.1.0",
    "gatsby": "~3.13.0",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-emotion": "^2.0.6",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-image": "^1.9.0",
    "gatsby-plugin-manifest": "^3.6.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-postcss": "4.8.0",
    "gatsby-plugin-react-helmet": "^4.6.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.6.9",
    "gatsby-plugin-sharp": "^3.9.0",
    "gatsby-plugin-typescript": "^3.12.0",
    "gatsby-source-filesystem": "^3.9.0",
    "gatsby-transformer-sharp": "^3.9.0",
    "git-branch": "^2.0.1",
    "glob": "^7.1.6",
    "informed": "^3.34.0",
    "isomorphic-git": "^1.10.1",
    "lodash": "^4.17.19",
    "md5-file": "^5.0.0",
    "mobx": "^6.0.0",
    "mobx-react": "^7.2.1",
    "null-loader": "^3.0.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.2.4",
    "process": "^0.11.10",
    "slash": "^2.0.0",
    "sleep-promise": "^8.0.1",
    "stream-browserify": "^3.0.0",
    "walk": "^2.3.14",
    "walkdir": "^0.4.1"
  },
  "devDependencies": {
    "tailwindcss": "^2.0.2"
  },
  "peerDependencies": {
    "gatsby": "~3.13.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
