{
  "name": "@bodiless/gatsby-theme-bodiless",
  "version": "1.0.0-beta.18",
  "description": "A Gatsby theme for BodilessJS",
  "author": "Chris Oden <coden@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "sideEffects": false,
  "bin": {
    "bl-validate-logs": "./bin/bl-validate-logs",
    "bl-clear-logs": "./bin/bl-clear-logs"
  },
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "/bin",
    "dist",
    "index.js",
    "index.d.ts",
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "queries.jsx",
    "Logger.js",
    "src/no-scroll-settings.json",
    "build-css.js",
    "create-node.js",
    "create-redirect-alias.js",
    "tailwind.base.config.js",
    "/doc",
    "bodiless.content.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run copy && tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api src",
    "clean": "rimraf -rf \"dist\" && rimraf index* && rimraf \"doc/api\"",
    "copy": "copyfiles -u 1 \"./src/dist/**/*.{json,jpg,png}\" \".\""
  },
  "dependencies": {
    "@bodiless/accordion": "^1.0.0-beta.18",
    "@bodiless/card": "^1.0.0-beta.18",
    "@bodiless/cli": "^1.0.0-beta.18",
    "@bodiless/components": "^1.0.0-beta.18",
    "@bodiless/components-ui": "^1.0.0-beta.18",
    "@bodiless/core": "^1.0.0-beta.18",
    "@bodiless/core-ui": "^1.0.0-beta.18",
    "@bodiless/fclasses": "^1.0.0-beta.18",
    "@bodiless/layouts": "^1.0.0-beta.18",
    "@bodiless/navigation": "^1.0.0-beta.18",
    "@bodiless/organisms": "^1.0.0-beta.18",
    "@bodiless/page": "^1.0.0-beta.18",
    "@bodiless/ui": "^1.0.0-beta.18",
    "@bodiless/webpack": "^1.0.0-beta.18",
    "@types/walk": "^2.3.0",
    "autoprefixer": "^10.4.2",
    "axios": "^0.21.0",
    "crypto-browserify": "^3.12.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "fast-glob": "^3.2.5",
    "find-up": "^4.1.0",
    "fs-extra": "^8.1.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-emotion": "^7.10.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-image": "^2.10.1",
    "gatsby-plugin-manifest": "^4.10.2",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-postcss": "5.10.0",
    "gatsby-plugin-react-helmet": "^5.10.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.7.0",
    "gatsby-plugin-sharp": "^4.11.1",
    "gatsby-plugin-typescript": "^4.10.1",
    "gatsby-source-filesystem": "^4.11.1",
    "gatsby-transformer-sharp": "^4.10.0",
    "git-branch": "^2.0.1",
    "glob": "^7.1.6",
    "informed": "^3.34.0",
    "isomorphic-git": "^1.10.1",
    "lodash": "^4.17.19",
    "md5-file": "^5.0.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.12",
    "process": "^0.11.10",
    "slash": "^2.0.0",
    "sleep-promise": "^8.0.1",
    "stream-browserify": "^3.0.0",
    "walk": "^2.3.14",
    "walkdir": "^0.4.1"
  },
  "devDependencies": {
    "@statoscope/webpack-plugin": "^5.20.1",
    "tailwindcss": "^3.0.23"
  },
  "peerDependencies": {
    "gatsby": "^4.11.2",
    "mobx": "^6.0.0",
    "mobx-react": "^7.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
