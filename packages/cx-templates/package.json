{
  "name": "@bodiless/cx-templates",
  "sideEffects": false,
  "version": "0.3.7",
  "description": "Provides designable templates for the CanvasX Design System",
  "author": "Andrei Beliayeu <abeliaye@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/doc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:api-doc",
    "build:api-doc": "typedoc --out doc/api ./src",
    "build:lib": "tsc -p ./tsconfig.json",
    "build:watch": "npm run build:lib -- --watch",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "@types/react-helmet": "^6.1.0",
    "react": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "dependencies": {
    "@bodiless/components": "^0.3.7",
    "@bodiless/core": "^0.3.7",
    "@bodiless/cx-elements": "^0.3.7",
    "@bodiless/cx-editors": "^0.3.7",
    "@bodiless/cx-layout": "^0.3.7",
    "@bodiless/fclasses": "^0.3.7",
    "@bodiless/gatsby-theme-bodiless": "^0.3.7",
    "lodash": "^4.17.19"
  }
}
