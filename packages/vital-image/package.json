{
  "name": "@bodiless/vital-image",
  "sideEffects": false,
  "version": "1.0.0-beta.7",
  "description": "Clean, designable Image components for Vital Design System based projects",
  "author": "Marco Pagliarulo <mpaglia1@its.jnj.com>",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/getDocs.js",
    "/assets",
    "/doc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api ./src",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "@bodiless/cli": "^1.0.0-beta.7",
    "@bodiless/components": "^1.0.0-beta.7",
    "@bodiless/components-ui": "^1.0.0-beta.7",
    "@bodiless/core": "^1.0.0-beta.7",
    "@bodiless/fclasses": "^1.0.0-beta.7",
    "@bodiless/gatsby-theme-bodiless": "^1.0.0-beta.7",
    "@bodiless/hydration": "^1.0.0-beta.7",
    "@bodiless/layouts": "^1.0.0-beta.7",
    "@bodiless/layouts-ui": "^1.0.0-beta.7",
    "@bodiless/vital-elements": "^1.0.0-beta.7",
    "lodash": "^4.17.19"
  }
}
