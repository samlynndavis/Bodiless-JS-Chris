{
  "name": "@bodiless/oidc",
  "version": "1.0.0-beta.18",
  "description": "Library to provide OpenID Connect (OIDC) and OAuth2 protocol support for client-side",
  "author": "Ivan Rudiuk <IRudiuk@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/getDocs.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api src",
    "lint": "tslint ./src/**/*.ts?x",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\""
  },
  "dependencies": {
    "@bodiless/cli": "^1.0.0-beta.18",
    "axios": "^0.21.0",
    "enzyme": "^3.9.0",
    "oidc-client-ts": "2.0.0-rc.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
