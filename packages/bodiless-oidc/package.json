{
  "name": "@bodiless/oidc",
  "version": "1.0.0-rc.38",
  "description": "Library to provide OpenID Connect (OIDC) and OAuth2 protocol support for client-side",
  "author": "Ivan Rudiuk <IRudiuk@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/getDocs.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-p build:lib",
    "build:lib": "tsc -p ./tsconfig.json",
    "build:watch": "npm run build:lib -- --watch",
    "build:api-doc": "typedoc --options ../../typedoc.js --out doc/api ./src",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\""
  },
  "dependencies": {
    "@bodiless/cli": "^1.0.0-rc.38",
    "axios": "^0.21.0",
    "oidc-client-ts": "2.0.0-rc.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "enzyme": "^3.9.0"
  }
}
