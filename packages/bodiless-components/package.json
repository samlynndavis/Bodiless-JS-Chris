{
  "name": "@bodiless/components",
  "version": "1.0.0-beta.1",
  "description": "Reusable compoments",
  "author": "Heidi Vanyo <hvanyo@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "sideEffects": [
    "*.css"
  ],
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/doc",
    "node-api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run copy && tsc -p ./tsconfig.json && npm run build:api-doc",
    "build:watch": "npm run build -- --watch",
    "build:api-doc": "typedoc --out doc/api src",
    "lint": "tslint ./src/**/*.ts?x",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\"",
    "copy": "copyfiles -u 1 \"./src/**/*.{css,png}\" \"./lib/\""
  },
  "dependencies": {
    "@bodiless/core": "^1.0.0-beta.1",
    "@bodiless/fclasses": "^1.0.0-beta.1",
    "@bodiless/layouts": "^1.0.0-beta.1",
    "@bodiless/ui": "^1.0.0-beta.1",
    "axios": "^0.21.0",
    "common-tags": "^1.8.0",
    "copyfiles": "^2.1.1",
    "debug": "^4.1.1",
    "enzyme": "^3.9.0",
    "informed": "^3.34.0",
    "lodash": "^4.17.19",
    "rc-tooltip": "^5.1.1",
    "react-contenteditable": "^3.2.6",
    "react-dropzone": "^10.1.5",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "@types/react-helmet": "^6.1.0",
    "mobx": "^6.0.0",
    "mobx-react": "^7.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/rc-tooltip": "^3.7.1",
    "@types/uuid": "^3.4.4"
  }
}
