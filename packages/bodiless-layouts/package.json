{
  "name": "@bodiless/layouts",
  "version": "1.0.0-rc.29",
  "description": "Layout managers for BodilessJS.",
  "author": "Erik Summerfield <esummer3@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "/lib",
    "/doc",
    "/bodiless.docs.json",
    "/getDocs.js",
    "tailwind.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-p build:lib && npm run build:api-doc",
    "build:lib": "tsc -p ./tsconfig.json",
    "build:watch": "npm run build:lib -- --watch",
    "build:api-doc": "typedoc --options ../../typedoc.js --out doc/api ./src",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\""
  },
  "peerDependencies": {
    "mobx-react": "^7.2.1",
    "react": "^17.0.2"
  },
  "dependencies": {
    "@bodiless/cli": "^1.0.0-rc.29",
    "@bodiless/core": "^1.0.0-rc.29",
    "@bodiless/fclasses": "^1.0.0-rc.29",
    "axios": "^0.21.0",
    "html2canvas": "^1.0.0-rc.3",
    "informed": "^3.34.0",
    "lodash": "^4.17.19",
    "md5-hex": "^4.0.0",
    "prop-types": "^15.7.2",
    "rc-tooltip": "^5.1.1",
    "re-resizable": "^4.11.0",
    "react-sortable-hoc": "^2.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/rc-tooltip": "^3.7.1",
    "enzyme": "^3.9.0"
  }
}
