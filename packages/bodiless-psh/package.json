{
  "name": "@bodiless/psh",
  "version": "1.0.0-rc.30",
  "description": "Provides cli tool for initializing a bodiless project on platform.sh",
  "author": "Chris Oden <coden@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "bin": {
    "bodiless-psh-init": "./lib/bodiless-psh-init.js"
  },
  "scripts": {
    "build": "run-p build:lib",
    "build:lib": "tsc -p ./tsconfig.json",
    "build:watch": "npm run build:lib -- --watch",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "/lib",
    "/resources",
    "/bodiless.docs.json",
    "/getDocs.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@bodiless/cli": "^1.0.0-rc.30",
    "copyfiles": "^2.1.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.19",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@types/copyfiles": "^2.1.1"
  }
}
