{
  "name": "@bodiless/tokens",
  "version": "1.0.0-rc.38",
  "description": "Utilities for browsing and organizing tokens.",
  "author": "Chris Oden <coden@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "/lib",
    "/doc",
    "/bodiless.docs.json",
    "/getDocs.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "run-p build:lib",
    "build:lib": "tsc -p ./tsconfig.json",
    "build:watch": "npm run build:lib -- --watch",
    "build:api-doc": "typedoc --options ../../typedoc.js --out doc/api ./src",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\""
  },
  "dependencies": {
    "@bodiless/core": "^1.0.0-rc.38",
    "@bodiless/data": "^1.0.0-rc.38",
    "@bodiless/fclasses": "^1.0.0-rc.38",
    "@bodiless/layouts": "^1.0.0-rc.38",
    "@bodiless/layouts-ui": "^1.0.0-rc.38",
    "crypto-js": "^4.1.1",
    "informed": "^4.44.0",
    "lodash": "^4.17.19",
    "md5-hex": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/crypto-js": "4.1.1"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
