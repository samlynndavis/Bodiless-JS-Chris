      "@bodiless/vital-card/*": ["vital-card/src/*"],
      "@bodiless/youtube": ["bodiless-youtube/src"],
      "@bodiless/youtube/*": ["bodiless-youtube/src/*"],
      "@bodiless/webpack": ["bodiless-webpack/src"],
      "@bodiless/webpack/*": ["bodiless-webpack/src/*"],
      "@bodiless/fclasses": ["fclasses/src"],
      "@bodiless/fclasses/*": ["fclasses/src/*"],
      "@bodiless/gatsby-theme-bodiless": ["gatsby-theme-bodiless/src"],
