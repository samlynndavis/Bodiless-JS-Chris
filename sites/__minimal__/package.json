{
  "name": "@sites/--minimal--",
  "version": "1.0.0-beta.2",
  "description": "Starter site for a basic Bodiless site",
  "author": "Chris Oden <coden@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "private": true,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "dependencies": {
    "@bodiless/backend": "^1.0.0-beta.2",
    "@bodiless/components": "^1.0.0-beta.2",
    "@bodiless/core": "^1.0.0-beta.2",
    "@bodiless/documentation": "^1.0.0-beta.2",
    "@bodiless/fclasses": "^1.0.0-beta.2",
    "@bodiless/gatsby-theme-bodiless": "^1.0.0-beta.2",
    "@bodiless/ui": "^1.0.0-beta.2",
    "autoprefixer": "^10.0.0",
    "docsify-cli": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gatsby": "~3.13.0",
    "postcss": "^8.2.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.0.2",
    "tailwindcss-dir": "^4.0.0"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "npm-run-all build:env-vars build:tailwind-configs build:lib build:doc",
    "build:doc": "bl-docs-build",
    "build:env-vars": "generate-env-vars",
    "build:lib": "bl-clear-logs && gatsby build && bl-validate-logs",
    "build:stats": "BODILESS_BUILD_STATS=1 npm run build",
    "build:tailwind-configs": "cross-env BODILESS_NPM_ROOT='.' generate-tailwind-configs",
    "check": "tsc -p ./tsconfig.check.json",
    "clean": "gatsby clean",
    "dev-backend": "node ./node_modules/@bodiless/backend/src/server.js",
    "dev-frontend": "gatsby develop -p 8000",
    "prepare": "node ../../scripts/set-missing-symlinks.js @bodiless",
    "prestart": "run-s build:env-vars build:tailwind-configs build:doc",
    "serve": "gatsby serve",
    "serve:docs": "docsify serve ./doc",
    "start": "run-p -r dev-frontend dev-backend"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^4.0.0"
  }
}
