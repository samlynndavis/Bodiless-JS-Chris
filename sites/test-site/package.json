  "scripts": {
    "build": "npm-run-all build:env-vars build:tailwind-configs build:lib build:scripts build:doc build:search",
    "build:doc": "bl-docs-build",
    "build:env-vars": "generate-env-vars",
    "build:lib": "bl-clear-logs && gatsby build && bl-validate-logs",
    "build:scripts": "tsc -p ./tsconfig.json",
    "build:search": "npm run search-index",
    "build:tailwind-configs": "BODILESS_NPM_ROOT='../..' generate-tailwind-configs",
    "check": "tsc -p ./tsconfig.check.json",
    "clean": "rimraf ./public/* ./.cache",
    "dev-backend": "node ./node_modules/@bodiless/backend/src/server.js",
    "dev-frontend": "gatsby develop -p 8005",
    "prepare": "node ../../scripts/set-missing-symlinks.js @bodiless",
    "prestart": "run-s build:env-vars build:tailwind-configs build:doc",
    "docs:serve": "docsify serve ./doc",
    "docs": "run-s build:doc docs:serve",
    "search-index": "create-search-index",
    "serve": "gatsby serve",
    "start": "run-p -r dev-frontend dev-backend"
  },
