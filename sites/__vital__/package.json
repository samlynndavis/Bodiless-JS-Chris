  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "npm-run-all build:env-vars build:lib build:doc build:search",
    "build:doc": "bl-docs-build",
    "build:env-vars": "bodiless generate-env-vars",
    "build:lib": "bl-clear-logs && gatsby build && bl-validate-logs",
    "build:search": "npm run search-index",
    "build:stats": "BODILESS_BUILD_STATS=1 npm run build",
    "check": "tsc -p ./tsconfig.check.json",
    "clean": "rimraf ./public/* ./.cache",
    "dev-backend": "bodiless-backend",
    "dev-frontend": "gatsby develop -p 8005",
    "prepare": "node ../../scripts/set-missing-symlinks.js @bodiless",
    "prestart": "run-s build:env-vars build:doc",
    "docs:serve": "docsify serve ./doc",
    "docs": "run-s build:doc docs:serve",
    "search-index": "create-search-index",
    "serve": "gatsby serve",
    "start": "run-p -r dev-frontend dev-backend"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.0",
    "gatsby-plugin-compile-es6-packages": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.23",
    "tailwindcss-dir": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^4.0.0"
